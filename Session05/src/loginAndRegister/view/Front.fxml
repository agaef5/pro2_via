<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginAndRegister.view.FrontViewController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="300.0" spacing="50.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="95.0" prefWidth="279.0" text="Welcome back" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Button fx:id="loginButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onLoginPressed" prefHeight="26.0" prefWidth="86.0" text="Login">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
            <Button fx:id="registerButton" layoutX="10.0" layoutY="134.0" mnemonicParsing="false" onAction="#onRegisterButtonPressed" prefHeight="26.0" prefWidth="86.0" text="Register">
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
